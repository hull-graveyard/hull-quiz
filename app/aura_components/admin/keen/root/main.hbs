<div class="tabbable">
  <div class="row pull-right">
    <div class="span12">
    {{#if options.quizId}}
      <a data-hull-action="reset" class="btn ben-default" href="#">See global stats</a>
    {{/if}}
    </div>
  </div>
  <ul class="nav nav-pills">
    <li class="active"><a data-toggle="tab" href="#count">Totals</a></li>
    <li><a data-toggle="tab" href="#series">Unique</a></li>
  </ul>
  <div class="tab-content">
    <div id="count" class="active tab-pane">
      <div class="row-fluid">
        <div class="span12">
          <h2>Visitors: </h2>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span5">
          <div
            data-hull-component='keen/number@admin'
            data-hull-event-collection='init'
            data-hull-analysis-type='count'
            data-hull-target-property='distinct_id'
            data-hull-label='Visitors'
            data-hull-filter='hull_app_id eq {{options.quizId}}'
            ></div>
        </div>
        <div class="span7">
          <div
            data-hull-component='keen/series@admin'
            data-hull-event-collection='init'
            data-hull-analysis-type='count'
            data-hull-target-property='distinct_id'
            data-hull-label='Visitors'
            data-hull-timeframe='this_month'
            data-hull-filter='hull_app_id eq {{options.quizId}}'
            ></div>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <h2>Games started: </h2>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span5">
          <div
            data-hull-component='keen/number@admin'
            data-hull-event-collection='hull.quiz.start'
            data-hull-analysis-type='count'
            data-hull-target-property='distinct_id'
            data-hull-label='Games started'
            data-hull-filter='hull_app_id eq {{options.quizId}}'
            ></div>
        </div>
        <div class="span7">
          <div
            data-hull-component='keen/series@admin'
            data-hull-event-collection='hull.quiz.start'
            data-hull-analysis-type='count'
            data-hull-target-property='distinct_id'
            data-hull-label='Games started'
            data-hull-timeframe='this_month'
            data-hull-filter='hull_app_id eq {{options.quizId}}'
            ></div>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span12">
          <h2>Games completed: </h2>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span5">
          <div
            data-hull-component='keen/number@admin'
            data-hull-event-collection='hull.achieve'

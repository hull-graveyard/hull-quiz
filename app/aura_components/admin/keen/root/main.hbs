            data-hull-timeframe='this_month'
            data-hull-filter='hull_app_id eq {{options.quizId}}'
            ></div>
        </div>
      </div>

      <div class="row-fluid">
        <div class="span12">
          <h2>Shares</h2>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span5">
          <div class="span4 span-offset-4">
            <div
              data-hull-component='keen/number@admin'
              data-hull-event-collection='hull.share.complete'
